#ordering
    list-style-type: none
    margin: 0
    padding: 0
    width: 60%

#ordering li 
    cursor: pointer
    text-align: center
    font-size: 11pt

#ordering li span 
    position: absolute
    margin-left: -1.3em

.answer_correct
    background-color: #33CC33!important

.answer_wrong
    background-color: #FF3333!important