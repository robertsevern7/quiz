$(document).ready(function() {
  $('#rhs > li').shuffle();

  $('#lhs > li').draggable();

  $('#rhs > li').droppable({
          drop: function( event, ui ) {
             var dropped = event.srcElement.textContent;
             var onto = event.target.textContent;
             alert(dropped + ' onto ' + onto);
          }
  });       
});